{
    "compilerOptions": {
        "lib": [
            "dom",
            "esnext"
        ],
        "esModuleInterop": true,
        "jsx": "react",
        "module": "esnext",
        "moduleResolution": "node",
        "strict": true,
        "sourceMap": true,
        "target": "esnext",
        "paths": {
            "@/*": [
                "./src/*"
            ]
        }
    },
    "include": [
        "src"
    ],
    "exclude": [
        "node_modules"
    ]
}